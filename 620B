#include<bits/stdc++.h>

using namespace std;
int nums[10]={6,2,5,5,4,5,6,3,7,6};

int main()
{
    int a,b;
    cin>>a;
    cin>>b;
    int sum=0;
    for(int i = a ; i <= b ;)
    {
        do
        {
            sum+= nums[a%10];
            a=a/10;
        }while(a != 0);
       i++;
       a= i ;
    }
    cout<<sum;

         return 0;
}
