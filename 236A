#include<bits/stdc++.h>

using namespace std;

int main()
{
    string name;
    cin >> name;
    unordered_set<char> distncit;
    int n = name.size();
    for(int i = 0 ; i < n ; i++ )
    {
        distncit.insert(name[i]);
    }
    n = distncit.size();
 
    if(n%2 == 0)
    {
        cout<<"CHAT WITH HER!";
    }
    else
    {
        cout<<"IGNORE HIM!";
    }
    return 0;
}
