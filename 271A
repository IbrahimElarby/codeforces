#include<bits/stdc++.h>

using namespace std;

int main()
{
  int n ;
  cin >> n ;
  vector<int>tt;
  unordered_set<int> sett;
  do{
sett.clear();
tt.clear();
 n++;
 int temp = n;
 while(temp!=0){

 sett.insert(temp%10);
 tt.push_back(temp%10);
 temp=temp/10;

 }



  }while(sett.size()!=4);
  for(int i = tt.size()-1 ; i>=0 ; i-- )
  {
      cout<<tt[i];
  }
    return 0;


}
