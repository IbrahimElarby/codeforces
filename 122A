#include<bits/stdc++.h>

using namespace std;
vector <int> lucky = {4,7,44,47,74,77,444,447,477,474,744,777,747,774} ;
int main()
{
  int n ;
  cin >> n ;
  int l =  lucky.size();
  bool cond = false;
  for(int i = 0 ; i < l ; i++)
  {
      if(n%lucky[i] == 0)
      {
          cout<<"YES";
          cond=true;
          break;

      }
  }
  if(cond==false)
  {
      cout<<"NO";
  }
    return 0;


}
